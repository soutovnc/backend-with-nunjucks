{% extends "layout.njk" %}

{% block head %}
    <title>Sobre - Rocketseat</title>
{% endblock %}

{% block content %}

<div id="wrapper">
    <img src="{{ about.logo_url }}" alt="Imagem de {{ about.name }}">
    <h1>{{ about.name }}</h1>
    <h2>{{ about.description }}</h2>
    
</div>

<div id="links-footer" class="links">
    {% for link in about.links %}
        <a href="{{ link.url }}" target="_blank">{{ link.name }}</a>
    {% endfor %}
</div>

{% endblock %}